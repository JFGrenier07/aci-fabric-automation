# Configuration ACI pour démonstration
# Copier vers .env et modifier avec vos valeurs

# Credentials ACI APIC
ACI_HOSTNAME=192.168.0.245
ACI_USERNAME=admin
ACI_PASSWORD=your_password_here

# Options de déploiement
ACI_VALIDATE_CERTS=false
GLOBAL_STATE=present
CSV_DIR=csv

# Timeouts et performance
ANSIBLE_TIMEOUT=300
ANSIBLE_FORKS=5
ANSIBLE_GATHERING=explicit